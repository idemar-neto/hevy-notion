{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ],
    "crons": [
        {
            "path": "/update_notion",
            "schedule": "0 58 1,12,23 ? * * *"
        }
    ]
}