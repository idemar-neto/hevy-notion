{
    "version": 2,
    "builds": [
      {
        "src": "api/update_notion.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/update_notion",
        "dest": "api/update_notion.py"
      }
    ],
    "crons": [
        {
          "path": "/api/update_notion",
          "schedule": "0 0 * * *"
        }
    ]
  }
  